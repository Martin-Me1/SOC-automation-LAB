{
  "name": "Mimikatz Detection Flow",
  "nodes": [
    {
      "app_name": "regex_capture_group",
      "name": "SHA256 Regex",
      "parameters": {
        "regex": "SHA256=([0-9A-Fa-f]{64})"
      }
    },
    {
      "app_name": "virustotal",
      "name": "Check Hash Reputation",
      "parameters": {
        "hash": "{{SHA256_Regex.group_0}}"
      }
    },
    {
      "app_name": "thehive",
      "name": "Create Alert",
      "parameters": {
        "title": "Mimikatz Detected",
        "description": "Triggered from Shuffle workflow",
        "severity": 2
      }
    }
  ]
}